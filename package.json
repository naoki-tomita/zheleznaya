{
  "name": "zheleznaya",
  "version": "0.1.19",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/naoki-tomita/zheleznaya",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "esbuild": "^0.15.12",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "esbuild",
    "lint": "npx prettier --write '{**,.}/*.ts'",
    "prepublish": "yarn lint && yarn build",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        { "jsxFactory": "h" }
      ]
    },
    "testPathIgnorePatterns": [
      "examples"
    ]
  }
}
